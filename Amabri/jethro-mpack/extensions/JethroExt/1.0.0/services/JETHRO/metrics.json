                    "metrics/jethro/jethro_mng/running_instances": {
                        "metric": "running_instances",
                        "pointInTime": true,
                        "temporal": true
                    },
                    "metrics/jethro/jethro_mng/running_instances_names": {
                        "metric": "running_instances_names",
