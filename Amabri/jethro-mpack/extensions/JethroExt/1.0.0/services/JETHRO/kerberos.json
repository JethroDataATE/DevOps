{
    "services": [{
        "name": "JETHRO",
        "identities": [{
            "name": "jethro_identity",
            "principal": {
                "configuration": "solr-config-env/solr_spnego_principal_name"
            },
            "keytab": {
                "file": "${keytab_dir}/jethro.service.keytab",
                "owner": {
                    "name": "jethro/service_user}",
                    "access": "r"
                },
                "configuration": "jethro-site/jethro.keytab.file"
            }
        }],
        "components": [{
                "name": "JETHRO_SERVER",
                "identities": [{
                    "name": "jethro_server_identity",
                    "principal": {
                        "value": "jethro/_HOST@${realm}",
                        "type": "service",
                        "local_username": "jethro"
                    },
                    "keytab": {
                        "file": "${keytab_dir}/jethro_server.service.keytab",
                        "owner": {
                            "name": "jethro",
                            "access": "r"
                        },
                        "configuration": "jethro-site/jethro_server.keytab.file"
                    }
                }]
            },
            {
                "name": "JETHRO_MAINT",
                "identities": [{
                    "name": "jethro_maint_identity",
                    "principal": {
                        "value": "jethro/_HOST@${realm}",
                        "type": "service",
                        "local_username": "jethro"
                    },
                    "keytab": {
                        "file": "${keytab_dir}/jethro_maint.service.keytab",
                        "owner": {
                            "name": "jethro",
                            "access": "r"
                        },
                        "configuration": "jethro-site/jethro_maint.keytab.file"
                    }
                }]
            },
            {
                "name": "JETHRO_LOAD_SCHEDULER",
                "identities": [{
                    "name": "jethro_loadscheduler_identity",
                    "principal": {
                        "value": "jethro/_HOST@${realm}",
                        "type": "service",
                        "local_username": "jethro"
                    },
                    "keytab": {
                        "file": "${keytab_dir}/jethro_loadscheduler.service.keytab",
                        "owner": {
                            "name": "jethro",
                            "access": "r"
                        },
                        "configuration": "jethro-site/jethro_loadscheduler.keytab.file"
                    }
                }]
            },
            {
                "name": "JETHRO_MNG",
                "identities": [{
                    "name": "jethro_manager_identity",
                    "principal": {
                        "value": "jethro/_HOST@${realm}",
                        "type": "service",
                        "local_username": "jethro"
                    },
                    "keytab": {
                        "file": "${keytab_dir}/jethro_jethromng.service.keytab",
                        "owner": {
                            "name": "jethro",
                            "access": "r"
                        },
                        "configuration": "jethro-site/jethro_jethromng.keytab.file"
                    }
                }]
            }
        ]
    }]
}