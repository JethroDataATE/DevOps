{
    "services": [{
        "name": "JETHRO",
        "identities": [{
                "name": "/smokeuser"
            },
            {
                "name": "jethrouser",
                "principal": {
                    "value": "${jethro-config/jethro_user}${principal_suffix}@${realm}",
                    "type": "user",
                    "configuration": "jethro-env/jethro.kerberos.principal",
                    "local_username": "${jethro-config/jethro_user}"
                },
                "keytab": {
                    "file": "${keytab_dir}/jethro.headless.keytab",
                    "owner": {
                        "name": "${jethro-config/jethro_user}",
                        "access": "r"
                    },
                    "group": {
                        "name": "${cluster-env/user_group}",
                        "access": ""
                    },
                    "configuration": "jethro-env/jethro.kerberos.keytab"
                }
            }
        ],
        "configurations": [{
                "jethro-env": {
                    "jethro.kerberos.enabled": "true"
                }
            },
            {
                "core-site": {
                    "hadoop.proxyuser.jethro.groups": "*",
                    "hadoop.proxyuser.jethro.hosts": "*"
                }
            }
        ]
    }]
}