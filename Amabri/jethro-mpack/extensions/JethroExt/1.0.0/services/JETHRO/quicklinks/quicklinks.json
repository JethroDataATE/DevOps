{
    "name": "default",
    "description": "default quick links configuration",
    "configuration": {
      "protocol": {
        "type": "http",
        "checks": [
          {
            "property": "solr_ssl_enable",
            "desired": "true",
            "site": "solr-ssl"
          }
        ]
      },
      "links": [
        {
          "name": "jethromng_ui",
          "label": "Jethro Manager UI",
          "component_name": "JETHRO_MNG",
          "url": "%@://%@:%@/",
          "requires_user_name": "false",
          "port": {
            "http_property": "jethromng_config_port",
            "http_default_port": "9100",
            "regex": "^(\\d+)$",
            "site": "jethromng-config-env"
          }
        }
      ]
    }
  }