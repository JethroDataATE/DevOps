{
    "layouts": [
        {
            "layout_name": "default_jethro_dashboard",
            "display_name": "Jethro Dashboard",
            "section_name": "JETHRO_SUMMARY",
            "widgetLayoutInfo": [
                {
                    "widget_name": "Running Instances",
                    "description": "Number of running instances in the cluster",
                    "widget_type": "NUMBER",
                    "is_visible": true,
                    "metrics": [
                      {
                        "name": "running_instances._min",
                        "metric_path": "metrics/jethro/running_instances._min",
                        "service_name": "JETHRO",
                        "component_name": "JETHRO_MNG"
                      }
                    ],
                    "values": [
                      {
                        "name": "Running Instances",
                        "value": "${running_instances._min}"
                      }
                    ],
                    "properties": {
                        "warning_threshold": "20",
                        "error_threshold": "50"
                    }
                }
            ]
        }
    ]
}